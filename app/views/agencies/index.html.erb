<p id="notice"><%= notice %></p>

<h1>Agencies</h1>

<table class='table leader-list'>
  <tr>
    <th></th>
    <th>Agency</th>
    <th>Updated</th>
  </tr>

  <% Agency.all.each_with_index do |agency, i| -%>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= link_to_name(agency) %></td>
      <td><small><%= updated_at(agency) %></small></td>
      <td><%= link_to_edit(agency) %></td>
    </tr>
  <% end -%>
</table>

<br>

<%= link_to 'Add a New Agency', new_agency_path %>
